{
  "name": "enterprise-ai-support-v14",
  "version": "14.0.0",
  "private": true,
  "description": "Enterprise AI Support V14 - Production-Ready Unified Version with Full Integration Suite",
  "scripts": {
    "dev": "next dev --turbopack -p 3014",
    "dev:full": "concurrently \"npm run dev:ws\" \"npm run dev\"",
    "dev:webhooks": "./scripts/start-dev.sh",
    "dev:stop": "./scripts/stop-dev.sh",
    "dev:webhook-url": "./scripts/get-webhook-url.sh",
    "dev:ws": "node server/mock-ws-server.js",
    "build": "next build --turbopack",
    "start": "next start -p 3014",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest __tests__/unit",
    "test:integration": "jest __tests__/integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:c-level": "playwright test tests/e2e/personas/c-level.spec.ts",
    "test:e2e:cs-manager": "playwright test tests/e2e/personas/cs-manager.spec.ts",
    "test:e2e:support-agent": "playwright test tests/e2e/personas/support-agent.spec.ts",
    "test:e2e:report": "playwright show-report tests/reports/playwright-report",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility",
    "test:security": "jest __tests__/security",
    "test:performance": "lighthouse http://localhost:3014 --preset=desktop --output=html --output-path=./tests/reports/lighthouse/report.html",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "motion": "^12.23.22",
    "next": "15.5.4",
    "prisma": "^6.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@swc/core": "^1.10.1",
    "@swc/jest": "^0.2.37",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.2.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
